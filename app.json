{
  "expo": {
    "name": "OxyLoans_App",
    "slug": "OxyLoans_App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/OxyLoansLogo1.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon23.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "css"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/OxyLoansLogo1.jpg",
        "backgroundColor": "#FFFFFF"
      },
      "package": "Oxyloan.App"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to take snap of you ."
        }
      ]
    ],
    "plugins": [
     [
       "expo-notifications",
       {
         "color": "#ffffff",
         "icon" : "./assets/oxylogo3.png"
       }
     ]
   ]
  }
}
